1. SELECT nombre FROM tblusuarios;
2. SELECT sexo, saldo FROM tblusuarios WHERE sexo = 'M' ORDER BY saldo DESC;
3. SELECT nombre, telefono, marca FROM tblusuarios WHERE marca = 'NOKIA' OR marca = 'BLACKBERRY' OR marca = 'SONY';
4. SELECT usuario FROM tblusuarios WHERE saldo = 0 OR saldo IS NULL;
5. SELECT * FROM tblusuarios WHERE nivel IN(1,2,3);
6. SELECT telefono FROM tblusuarios WHERE saldo <= 300;
7. SELECT SUM(saldo) FROM tblusuarios WHERE compaÃ±ia = 'NEXTEL';
