SELECT nombre FROM tblusuarios;
SELECT sexo, saldo FROM tblusuarios WHERE sexo = 'M' ORDER BY saldo DESC;
SELECT nombre, telefono, marca FROM tblusuarios WHERE marca = 'NOKIA' OR marca = 'BLACKBERRY' OR marca = 'SONY';
SELECT usuario FROM tblusuarios WHERE saldo = 0 OR saldo IS NULL;
SELECT * FROM tblusuarios WHERE nivel IN(1,2,3);
SELECT telefono FROM tblusuarios WHERE saldo <= 300;
SELECT SUM(saldo) FROM tblusuarios WHERE compaÃ±ia = 'NEXTEL';
